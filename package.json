{
  "name": "gql-doc",
  "displayName": "gqlDoc",
  "description": "",
  "author": "never-w",
  "publisher": "never-w",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gqlDoc.start",
        "title": "Start new gql doc",
        "category": "gqlDoc"
      },
      {
        "command": "gqlDoc.settings",
        "title": "Settings gql util",
        "category": "settings gql util"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gql-doc",
          "title": "Gql工具",
          "icon": "assets/logo/qiufen-logo.svg"
        }
      ]
    },
    "views": {
      "gql-doc": [
        {
          "id": "gql-util",
          "name": "功能",
          "visibility": "visible"
        },
        {
          "id": "gql-settings",
          "visibility": "visible",
          "name": "Settings"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "gql-util",
        "contents": "[🏠️ 拉取Gql Doc](command:gqlDoc.start)"
      },
      {
        "view": "gql-settings",
        "contents": "[🛠 打开配置页](command:gqlDoc.settings)"
      }
    ],
    "configuration": {
      "properties": {
        "gql-doc.port": {
          "type": "number",
          "default": 9400,
          "description": "service port"
        },
        "gql-doc.url": {
          "type": "string",
          "default": "",
          "description": "backend service url"
        },
        "gql-doc.localSchemaFile": {
          "type": "string",
          "default": "",
          "description": "local graphql schema file path"
        },
        "gql-doc.schemaPolicy": {
          "type": "string",
          "default": "remote",
          "description": "use either local schema or remote schema, if unset, remote will be used.  value: 'local' | 'remote'"
        },
        "gql-doc.mock": {
          "type": "object",
          "default": {
            "enable": true,
            "mockDirectiveDefaultEnableValue": false,
            "context": {
              "Random": true
            },
            "scalarMap": {
              "Int": "() => Random.integer(0, 100)",
              "String": "() => Random.ctitle(2, 4)",
              "ID": "() => Random.id()",
              "Boolean": "() => Random.boolean()",
              "BigDecimal": "() => Random.integer(0, 1000000)",
              "Float": "() => Random.float(0, 100)",
              "Date": "() => Random.date()",
              "DateTime": "() => Random.datetime()",
              "Long": "() => Random.integer(0, 10000)",
              "NumberOrBoolOrStringOrNull": "() => null",
              "NumberOrString": "() => null"
            },
            "resolvers": {
              "Query": {}
            }
          },
          "description": "mock config"
        },
        "gql-doc.Authorization": {
          "type": "string",
          "default": "",
          "description": "app token"
        },
        "gql-doc.playground": {
          "type": "object",
          "default": {
            "headers": {
              "appversioncode": 33,
              "app-version": 33,
              "platform": ""
            }
          },
          "description": " playground config/request headers"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "bufferutil": "^4.0.7",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.28.0",
    "file-loader": "^6.2.0",
    "glob": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "mocha": "^10.1.0",
    "style-loader": "^3.3.1",
    "style-resources-loader": "^1.5.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "utf-8-validate": "^6.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@fruits-chain/hooks-laba": "^1.0.2",
    "@fruits-chain/qiufen-helpers": "^1.0.1",
    "@graphql-tools/load": "^7.8.8",
    "@graphql-tools/url-loader": "^7.16.29",
    "antd": "^5.1.2",
    "classnames": "^2.3.2",
    "clipboard": "^2.0.11",
    "cosmiconfig": "^8.0.0",
    "graphql": "^16.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "zustand": "3.7.2"
  }
}
